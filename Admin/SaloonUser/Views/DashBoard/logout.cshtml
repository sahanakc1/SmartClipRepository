
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <title>Smart Clips</title>
    @*<link rel="shortcut icon" href="~/assets/images/favicon.ico">*@

    @Styles.Render("~/Content/css")

</head>


<body>


    <!-- Begin page -->
    <div class="accountbg"></div>
    <div class="wrapper-page">

        <div class="card">
            <div class="card-body">

                <h3 class="text-center mt-0 m-b-15">                   
                    <a href="@Url.Action("Login", "Login")" class="logo logo-admin">
                        <i class="mdi mdi-assistant"></i><span> Smart Clips</span>
                    </a>
                </h3>

                <div class="p-3">
                    <div class="text-center">
                        <h5><i class="mdi mdi-lock mr-1 text-danger"></i>Logged Out</h5>
                        <p class="text-muted pt-2 font-14">Thank You </p>
                    </div>
                    <div class="form-group text-center row m-t-20">
                        <div class="col-12">                            
                            <a href="@Url.Action("Login", "Login")" class="btn btn-danger btn-block waves-effect waves-light text-white">
                                
                            </a>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>



    <!-- jQuery  -->
    <script src="~/assets/js/jquery.min.js"></script>
    <script src="~/assets/js/popper.min.js"></script>
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/modernizr.min.js"></script>
    <script src="~/assets/js/waves.js"></script>
    <script src="~/assets/js/jquery.slimscroll.js"></script>
    <script src="~/assets/js/jquery.nicescroll.js"></script>
    <script src="~/assets/js/jquery.scrollTo.min.js"></script>

    <!-- App js -->
    <script src="~/assets/js/app.js"></script>

</body>
</html>